def decidable_lt :
  cts.Term
    cts.star
    (cts.prod
       cts.box
       cts.star
       cts.star
       cts.I
       nat.nat
       (x =>
        cts.prod cts.box cts.star cts.star cts.I nat.nat (x0 => decidable.decidable (lt.lt x x0))))
  :=
  n:(cts.Term cts.box nat.nat) => m:(cts.Term cts.box nat.nat) => decidable_le.decidable_le (S.S n) m.

