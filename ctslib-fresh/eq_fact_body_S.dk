def eq_fact_body_S :
  cts.Term
    cts.star
    (cts.prod
       cts.box
       cts.star
       cts.star
       cts.I
       nat.nat
       (x =>
        leibniz.leibniz nat.nat (fact_body.fact_body (S.S x)) (times.times (fact.fact x) (S.S x))))
  :=
  n:(cts.Term cts.box nat.nat) =>
  equal_leibniz.equal_leibniz
    nat.nat
    (fact_body.fact_body (S.S n))
    (times.times (fact.fact n) (S.S n))
    (axiom_fact_body_S.axiom_fact_body_S n).

